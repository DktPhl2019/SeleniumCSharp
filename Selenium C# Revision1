        [Test]
        public static void method5()
        {
            System.Threading.Thread.Sleep(mysecs);
            String state1 = "California";
            String state2 = "Florida";
            String state3 = "New Jersey";
            String state4 = "New York";
            String state5 = "Ohio";
            String state6 = "Texas";
            String state7 = "Pennsylvania";
            String state8 = "Washington";
            IWebElement MultiSelect = driver.FindElement(By.Id("multi-select"));
            IWebElement FirstSelectedButton = driver.FindElement(By.Id("printMe"));
            IWebElement GetAllSelectedButton = driver.FindElement(By.Id("printAll"));
            IWebElement checkmsg1 = driver.FindElement(By.ClassName("getall-selected"));

            String frstoutstate1 = "First selected option is : California";
            String frstoutstate2 = "First selected option is : Florida";
            String frstoutstate3 = "First selected option is : New Jersey";
            String frstoutstate4 = "First selected option is : New York";
            String frstoutstate5 = "First selected option is : Ohio";
            String frstoutstate6 = "First selected option is : Texas";
            String frstoutstate7 = "First selected option is : Pennsylvania";
            String frstoutstate8 = "First selected option is : Washington";

            String optoutstate1 = "Options selected are : California";
            String optoutstate2 = "Options selected are : Florida";
            String optoutstate3 = "Options selected are : New Jersey";
            String optoutstate4 = "Options selected are : New York";
            String optoutstate5 = "Options selected are : Ohio";
            String optoutstate6 = "Options selected are : Texas";
            String optoutstate7 = "Options selected are : Pennsylvania";
            String optoutstate8 = "Options selected are : Washington";
            System.Threading.Thread.Sleep(mysecs);
            GetAllSelectedButton.Click();

            Assert.That(checkmsg1.Text, Does.Contain(state2), "'{0}' does not contain '{1}'", checkmsg1.Text, state2);

            if (checkmsg1.Text.Contains(state2))
            {
                Console.WriteLine("The output expected: " + optoutstate1 + "\n" + state2);
                Console.WriteLine("The output shown: " + checkmsg1.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + optoutstate1 + "\n" + optoutstate2);
                Console.WriteLine("The output shown: " + checkmsg1.Text);
            }

            //return from Select Dropdown List to Main Page
            driver.Navigate().Back();
            System.Threading.Thread.Sleep(mysecs);
        }
    }

    public class Test104 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            System.Threading.Thread.Sleep(mysecs);
            //Open Input Form Submit

            IWebElement InputForms5 = driver.FindElement(By.LinkText("Input Forms"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms5.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Input Form Submit")).Click();

            String firstname = "Jack";
            String lastname = "Smith";
            String email = "jack.smith@yahoo.com";
            String phone = "215 - 999 - 9901";
            String address = "1100 Chestnut Street";
            String city = "Philadelphia";
            String zipcode = "19106";
            String websiteurl = "www.phily.com";
            String mycomment = "This is my comment";
            driver.FindElement(By.Name("first_name")).SendKeys(firstname);
            driver.FindElement(By.Name("last_name")).SendKeys(lastname);
            driver.FindElement(By.Name("email")).SendKeys(email);
            driver.FindElement(By.Name("phone")).SendKeys(phone);
            driver.FindElement(By.Name("address")).SendKeys(address);
            driver.FindElement(By.Name("city")).SendKeys(city);
            IWebElement mystate1 = driver.FindElement(By.CssSelector("select.form-control"));
            SelectElement pickstate = new SelectElement(mystate1);
            System.Threading.Thread.Sleep(mysecs);
            pickstate.SelectByText("Pennsylvania");
            driver.FindElement(By.Name("zip")).SendKeys(zipcode);
            driver.FindElement(By.Name("website")).SendKeys(websiteurl);

            IWebElement radio11 = driver.FindElement(By.XPath("//input[contains(@value,'yes')]"));
            IWebElement radio21 = driver.FindElement(By.XPath("//input[contains(@value,'no')]"));
            System.Threading.Thread.Sleep(mysecs);
            radio11.Click();
            System.Threading.Thread.Sleep(mysecs);
            radio21.Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.Name("comment")).SendKeys(mycomment);
            IWebElement mybtn11 = driver.FindElement(By.XPath("//button[contains(@type,'submit')]"));
            System.Threading.Thread.Sleep(mysecs);
            mybtn11.Click();
        }
    }

    public class Test105 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Ajax Form Submit

            IWebElement InputForms6 = driver.FindElement(By.LinkText("Input Forms"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms6.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Ajax Form Submit")).Click();

            String myname = "First Test";
            String mytextarea = "Enter long sentence into the text area";
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.Id("title")).SendKeys(myname);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.Id("description")).SendKeys(mytextarea);
            IWebElement mysubmitbtn = driver.FindElement(By.Id("btn-submit"));
            System.Threading.Thread.Sleep(mysecs);
            mysubmitbtn.Click();

            String expsubmittext = "Form submited Successfully!";
            IWebElement mysubmittext = driver.FindElement(By.Id("submit-control"));

            Assert.That(mysubmittext.Text, Does.Contain(expsubmittext), "'{0}' does not contain '{1}'", mysubmittext.Text, expsubmittext);

            if (mysubmittext.Text.Contains(expsubmittext))
            {
                Console.WriteLine("The output expected: " + expsubmittext);
                Console.WriteLine("The output shown: " + mysubmittext.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + expsubmittext);
                Console.WriteLine("The output shown: " + mysubmittext.Text);
            }
        }
    }

