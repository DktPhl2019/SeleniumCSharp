            System.Threading.Thread.Sleep(mysecs);
            IWebElement Selectfile = driver.FindElement(By.Id("files"));
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.Click();
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            Selectfile.SendKeys(Keys.Enter);
        }
