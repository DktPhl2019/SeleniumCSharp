            if (checkmsg1.Text.Contains(state1))
            {
                Console.WriteLine("The output expected: " + frstoutstate1);
                Console.WriteLine("The output shown: " + checkmsg1.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + frstoutstate1);
                Console.WriteLine("The output shown: " + checkmsg1.Text);
            }
        }

