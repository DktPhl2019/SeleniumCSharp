        [Test]
        public static void method3()
        {
            System.Threading.Thread.Sleep(mysecs);
            IWebElement InputBox1 = driver.FindElement(By.XPath("//input[contains(@data-action,'filter')]"));
            String mytest3 = "completed";
            System.Threading.Thread.Sleep(mysecs);
            InputBox1.SendKeys(mytest3);
            System.Threading.Thread.Sleep(mysecs);
            InputBox1.SendKeys(Keys.Enter);

