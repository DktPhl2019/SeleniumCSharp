        [Test]
        public static void method3()
        {
            System.Threading.Thread.Sleep(mysecs);
            String mymessage3 = "Showing 1 to 10 of 32 entries";
            IWebElement mymessagebox = driver.FindElement(By.XPath("//div[@class='dataTables_info']"));
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).SendKeys(Keys.ArrowUp);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).SendKeys(Keys.Enter);

            Assert.That(mymessagebox.Text, Does.Contain(mymessage3), "'{0}' does not contain '{1}'", mymessagebox.Text, mymessage3);

            if (mymessagebox.Text.Contains(mymessage3))
            {
                Console.WriteLine("The output expected: " + mymessage3);
                Console.WriteLine("The output shown: " + mymessagebox.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + mymessage3);
                Console.WriteLine("The output shown: " + mymessagebox.Text);
            }

            //next
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[contains(.,'Next')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@class='paginate_button '][contains(.,'3')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[contains(.,'Next')]")).Click();
            System.Threading.Thread.Sleep(mysecs);

            //prior
            driver.FindElement(By.XPath("//a[contains(.,'Previous')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@class='paginate_button '][contains(.,'2')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[contains(.,'Previous')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[contains(.,'Previous')]")).Click();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).SendKeys("100");
            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).SendKeys(Keys.Enter);

            String myname111 = "A.";
            String myposition = "Integration Specialist";
            String myoffice = "London";
            String myage = "30";
            String mystartdate = "Dec";
            String mysalary = "500";

            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(myname111);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).Clear();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(myposition);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).Clear();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(myoffice);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).Clear();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(myage);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).Clear();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(mystartdate);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).Clear();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(mysalary);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//input[@type='search']")).Clear();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//input[@type='search']")).SendKeys(Keys.Clear); ;
            System.Threading.Thread.Sleep(mysecs);
            //Click on column headings to sort
            driver.FindElement(By.XPath("//th[contains(.,'Name')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//th[contains(.,'Position')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//th[contains(.,'Office')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//th[contains(.,'Age')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//th[contains(.,'Start date')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//th[contains(.,'Salary')]")).Click();
        }
    }

