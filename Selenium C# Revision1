    public class Test110 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method1()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Table Data Search

            IWebElement InputForms11 = driver.FindElement(By.LinkText("Table"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms11.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Table Data Search")).Click();

            String mytest1 = "Wireframes";
            String mytest2 = "Jane Doe";
            String mytest3 = "completed";

            IWebElement InputBox1 = driver.FindElement(By.XPath("//input[contains(@data-action,'filter')]"));
            System.Threading.Thread.Sleep(mysecs);
            InputBox1.Click();
            System.Threading.Thread.Sleep(mysecs);
            InputBox1.SendKeys(mytest1);
            System.Threading.Thread.Sleep(mysecs);
            InputBox1.SendKeys(Keys.Enter);

            IWebElement OutBox1 = driver.FindElement(By.XPath("//td[contains(.,'Wireframes')]"));

            Assert.That(OutBox1.Text, Does.Contain(mytest1), "'{0}' does not contain '{1}'", OutBox1.Text, mytest1);

            if (OutBox1.Text.Contains(mytest1))
            {
                Console.WriteLine("The output expected: " + mytest1);
                Console.WriteLine("The output shown: " + OutBox1.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + mytest1);
                Console.WriteLine("The output shown: " + OutBox1.Text);
            }

            InputBox1.Clear();
        }

        [Test]
