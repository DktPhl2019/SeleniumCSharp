            InputBox1.SendKeys(Keys.Enter);
        }

        [Test]
        public static void method4()
        {
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//button[contains(.,'Filter')]")).Click();

            String mytest4 = "3";
            String mytest5 = "jacobs";
            String mytest6 = "Byron";
            String mytest7 = "Samuels";

            IWebElement InputBox11 = driver.FindElement(By.XPath("//input[contains(@placeholder,'#')]"));
            System.Threading.Thread.Sleep(mysecs);
            InputBox11.Click();
            System.Threading.Thread.Sleep(mysecs);
            InputBox11.SendKeys(mytest4);
            System.Threading.Thread.Sleep(mysecs);
            InputBox11.SendKeys(Keys.Enter);

            IWebElement OutBox11 = driver.FindElement(By.XPath("(//td[contains(.,'3')])[3]"));

            Assert.That(OutBox11.Text, Does.Contain(mytest4), "'{0}' does not contain '{1}'", OutBox11.Text, mytest4);

            if (OutBox11.Text.Contains(mytest4))
            {
                Console.WriteLine("The output expected: " + mytest4);
                Console.WriteLine("The output shown: " + OutBox11.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + mytest4);
                Console.WriteLine("The output shown: " + OutBox11.Text);
            }

            InputBox11.Clear();
        }

