            IWebElement elementmovetoleft = driver.FindElement(By.XPath("//button[contains(@class,'btn btn-default btn-sm move-left')]"));

            IWebElement elementright1 = driver.FindElement(By.XPath("//li[@class='list-group-item'][contains(.,'Cras justo odio')]"));
            IWebElement elementright2 = driver.FindElement(By.XPath("(//li[@class='list-group-item'][contains(.,'Dapibus ac facilisis in')])[2]"));
            IWebElement elementright3 = driver.FindElement(By.XPath("(//li[@class='list-group-item'][contains(.,'Morbi leo risus')])[2]"));
            IWebElement elementright4 = driver.FindElement(By.XPath("(//li[@class='list-group-item'][contains(.,'Porta ac consectetur ac')])[2]"));
            IWebElement elementright5 = driver.FindElement(By.XPath("(//li[@class='list-group-item'][contains(.,'Vestibulum at eros')])[2]"));
