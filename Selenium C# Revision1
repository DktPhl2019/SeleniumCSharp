            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//button[@type='button'])[2]")).Click();

            IWebElement OneHundredPercnt = driver.FindElement(By.XPath("//div[@class='percenttext']"));

            Assert.That(OneHundredPercnt.Text, Does.Contain("100%"), "'{0}' does not contain '{1}'", OneHundredPercnt.Text, "100%");

            if (OneHundredPercnt.Text.Contains("100%"))
            {
                Console.WriteLine("The output expected: " + "100%");
                Console.WriteLine("The output shown: " + OneHundredPercnt.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + "100%");
                Console.WriteLine("The output shown: " + OneHundredPercnt.Text);
            }
        }
    }

    public class Test116 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Drag & Drop Sliders

            IWebElement InputForms17 = driver.FindElement(By.LinkText("Progress Bars"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms17.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Drag & Drop Sliders")).Click();

            //grey
            Actions greyslider = new Actions(driver);
            IWebElement greybar = driver.FindElement(By.XPath("//input[@onchange='range.value=value']"));
            int numbergreybar1 = 50;
            int numbergreybar2 = -30;
            greyslider.MoveToElement(greybar).ClickAndHold().MoveByOffset(numbergreybar1, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
            greyslider.MoveToElement(greybar).ClickAndHold().MoveByOffset(numbergreybar2, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);

            //green
            Actions greenslider = new Actions(driver);
            IWebElement greenbar = driver.FindElement(By.XPath("//input[@onchange='rangeSuccess.value=value']"));
            int numbergreenbar1 = 60;
            int numbergreenbar2 = -40;
            greenslider.MoveToElement(greenbar).ClickAndHold().MoveByOffset(numbergreenbar1, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
            greenslider.MoveToElement(greenbar).ClickAndHold().MoveByOffset(numbergreenbar2, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);

            //orange
            Actions orangeslider = new Actions(driver);
            IWebElement orangebar = driver.FindElement(By.XPath("//input[@onchange='rangeWarning.value=value']"));
            int numberorangebar1 = 80;
            int numberorangebar2 = -50;
            orangeslider.MoveToElement(orangebar).ClickAndHold().MoveByOffset(numberorangebar1, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
            orangeslider.MoveToElement(orangebar).ClickAndHold().MoveByOffset(numberorangebar2, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);

            //dark blue
            Actions darkblueslider = new Actions(driver);
            IWebElement darkbluebar = driver.FindElement(By.XPath("//input[@onchange='rangePrimary.value=value']"));
            int numberdarkbluebar1 = 70;
            int numberdarkbluebar2 = -80;
            darkblueslider.MoveToElement(darkbluebar).ClickAndHold().MoveByOffset(numberdarkbluebar1, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
            darkblueslider.MoveToElement(darkbluebar).ClickAndHold().MoveByOffset(numberdarkbluebar2, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);

            //light blue
            Actions lightblueslider = new Actions(driver);
            IWebElement lightbluebar = driver.FindElement(By.XPath("//input[@onchange='rangeInfo.value=value']"));
            int numberlightbluebar1 = 60;
            int numberlightbluebar2 = -80;
            lightblueslider.MoveToElement(lightbluebar).ClickAndHold().MoveByOffset(numberlightbluebar1, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
            lightblueslider.MoveToElement(lightbluebar).ClickAndHold().MoveByOffset(numberlightbluebar2, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);

            //red
            Actions redslider = new Actions(driver);
            IWebElement redbar = driver.FindElement(By.XPath("//input[@onchange='rangeDanger.value=value']"));
            int numberredbar1 = 50;
            int numberredbar2 = -90;
            redslider.MoveToElement(redbar).ClickAndHold().MoveByOffset(numberredbar1, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
            redslider.MoveToElement(redbar).ClickAndHold().MoveByOffset(numberredbar2, 0).Release().Perform();
            System.Threading.Thread.Sleep(mysecs);
        }
    }

    public class Test117 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method1()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Bootstrap Alerts

            IWebElement InputForms18 = driver.FindElement(By.LinkText("Alerts & Modals"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms18.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Bootstrap Alerts")).Click();

            String m1 = "I'm an autocloseable success message. I will hide in 5 seconds.";
            String m2 = "I'm a normal success message. To close use the appropriate button.";
            String m3 = "I'm an autocloseable warning message. I will hide in 3 seconds.";
            String m4 = "I'm a normal warning message. To close use the appropriate button.";
            String m5 = "I'm an autocloseable danger message. I will hide in 5 seconds.";
            String m6 = "I'm a normal danger message. To close use the appropriate button.";
            String m7 = "I'm an autocloseable info message. I will hide in 6 seconds.";
            String m8 = "I'm a normal info message. To close use the appropriate button.";
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//button[@id='autoclosable-btn-success']")).Click();
            IWebElement alert1 = driver.FindElement(By.XPath("//div[contains(@class,'alert alert-success alert-autocloseable-success')]"));

            Assert.That(alert1.Text, Does.Contain(m1), "'{0}' does not contain '{1}'", alert1.Text, m1);

            if (alert1.Text.Contains(m1))
            {
                Console.WriteLine("The output expected: " + m1);
                Console.WriteLine("The output shown: " + alert1.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + m1);
                Console.WriteLine("The output shown: " + alert1.Text);
            }

            System.Threading.Thread.Sleep(mysecs);
        }

