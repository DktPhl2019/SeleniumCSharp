using ExcelDataReader;
using NUnit.Framework;
using OpenQA.Selenium.Chrome;
using OpenQA.Selenium.Interactions;
using OpenQA.Selenium.Support.UI;
using OpenQA.Selenium;
using System.Collections.Generic;
using System.Data;
using System.IO;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System;

namespace Selenium_C_Sharp_NUNIT
{

        public class InitialSetup
    {
        public static IWebDriver driver = new ChromeDriver();
        public static int mysecs = 1500;
    }
    
    public class Test100:InitialSetup
    {
        
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method1()
        {
            myMethod();
            IWebElement popuptoclose = driver.FindElement(By.CssSelector(".at-cm-no-button"));
            popuptoclose.Click();
            System.Threading.Thread.Sleep(mysecs);
            IWebElement InputForms = driver.FindElement(By.LinkText("Input Forms"));
            InputForms.SendKeys(Keys.Down); //Select Input Form
            System.Threading.Thread.Sleep(mysecs);
            IWebElement SimpleFormDemo = driver.FindElement(By.LinkText("Simple Form Demo"));
            SimpleFormDemo.Click(); // Select Simple Forms Demo from Input Form
            IWebElement myinput = driver.FindElement(By.Id("user-message"));
            String input1 = "This is the first input box";
            myinput.SendKeys(input1); // enter text
            System.Threading.Thread.Sleep(mysecs);
            IWebElement mybutton1 = driver.FindElement(By.CssSelector("button.btn:nth-child(2)"));
            mybutton1.Click(); // click button
            System.Threading.Thread.Sleep(mysecs);
            IWebElement output1 = driver.FindElement(By.Id("display"));
            Assert.That(output1.Text, Does.Contain(input1), "'{0}' does not contain '{1}'", output1.Text, input1); //assert text matches text entered
            
            if (output1.Text.Contains(input1))
            {
                Console.WriteLine("Your Input text matched");
                Console.WriteLine("The input expected: " + input1);
                Console.WriteLine("The input shown: " + output1.Text);
            }
            else
            {
                Console.WriteLine("Your Input text did not match");
                Console.WriteLine("The input expected: " + input1);
                Console.WriteLine("The input shown: " + output1.Text);
            }
        }

        [Test]
        public static void method2()
        {
            System.Threading.Thread.Sleep(mysecs);
            int num1 = 100;
            int num2 = 200;
            int sum1 = num1 + num2;
            System.Threading.Thread.Sleep(mysecs);
            string msum1 = Convert.ToString(sum1);
            System.Threading.Thread.Sleep(mysecs);
            IWebElement el1 = driver.FindElement(By.Id("sum1"));
            IWebElement el2 = driver.FindElement(By.Id("sum2"));
            IWebElement mybutton2 = driver.FindElement(By.CssSelector("button.btn:nth-child(3)"));
            IWebElement output2 = driver.FindElement(By.Id("displayvalue"));
            System.Threading.Thread.Sleep(mysecs);
            el1.SendKeys("" + num1);//enter num1
            System.Threading.Thread.Sleep(mysecs);
            el2.SendKeys("" + num2);//enter num2
            System.Threading.Thread.Sleep(mysecs);
            mybutton2.Click();//button click
            System.Threading.Thread.Sleep(mysecs);
            Assert.That(output2.Text, Does.Contain(msum1), "'{0}' does not contain '{1}'", output2.Text, msum1);// assert sum matches el1 and el2

            if (output2.Text.Contains(msum1))
            {
                Console.WriteLine("The sum is correct");
                Console.WriteLine("The sum expected: " + sum1);
                Console.WriteLine("The sum calculated: " + output2.Text);
            }
            else
            {
                Console.WriteLine("The sum is not correct");
                Console.WriteLine("The sum expected: " + sum1);
                Console.WriteLine("The sum calculated: " + output2.Text);
            }
        }
    }

    public class Test101 : InitialSetup
    {
        
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }
        [Test]
        public static void method1()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            IWebElement InputForms2 = driver.FindElement(By.LinkText("Input Forms"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms2.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Checkbox Demo")).Click();
            //Click On Checkbox
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.Id("isAgeSelected")).Click();
            //Verify expected text
            String input3 = "Success - Check box is checked";
            IWebElement output3 = driver.FindElement(By.Id("txtAge"));

            Assert.That(output3.Text, Does.Contain(input3), "'{0}' does not contain '{1}'", output3.Text, input3);

            if (output3.Text.Contains(input3))
            {
                Console.WriteLine("Your Input text matched");
                Console.WriteLine("The input expected: " + input3);
                Console.WriteLine("The input shown: " + output3.Text);
            }
            else
            {
                Console.WriteLine("Your Input text did not match");
                Console.WriteLine("The input expected: " + input3);
                Console.WriteLine("The input shown: " + output3.Text);
            }
        }

        [Test]
        public static void method2()

        {
            System.Threading.Thread.Sleep(mysecs);
            //Uncheck the checkbox
            driver.FindElement(By.Id("isAgeSelected")).Click();

            String input4 = "";
            IWebElement output4 = driver.FindElement(By.Id("txtAge"));

            Assert.That(output4.Text, Does.Contain(input4), "'{0}' does not contain '{1}'", output4.Text, input4);

            if (output4.Text.Contains(input4))
            {
                Console.WriteLine("Your Input text matched");
                Console.WriteLine("The input expected: " + input4);
                Console.WriteLine("The input shown: " + output4.Text);
            }
            else
            {
                Console.WriteLine("Your Input text did not match");
                Console.WriteLine("The input expected: " + input4);
                Console.WriteLine("The input shown: " + output4.Text);
            }

            //4 checkboxes test

            IWebElement buttonchecktext = driver.FindElement(By.Id("check1"));
            IWebElement checkBox1 = driver.FindElement(By.XPath("//label[contains(.,'Option 1')]"));
            IWebElement checkBox2 = driver.FindElement(By.XPath("//label[contains(.,'Option 2')]"));
            IWebElement checkBox3 = driver.FindElement(By.XPath("//label[contains(.,'Option 3')]"));
            IWebElement checkBox4 = driver.FindElement(By.XPath("//label[contains(.,'Option 4')]"));

            if ((!checkBox1.Selected) && (!checkBox2.Selected) && (!checkBox3.Selected) && (!checkBox4.Selected))
            {
                Console.WriteLine("No checkboxes selected: ");
                Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));
            }

            if (!checkBox1.Selected)
            {
                checkBox1.Click();
                Console.WriteLine("1st checkbox is selected: ");
                Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));
            }

            if (!checkBox2.Selected)
            {
                checkBox2.Click();
                Console.WriteLine("1st and 2nd checkboxes selected: ");
                Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));
            }

            if (!checkBox3.Selected)
            {
                checkBox3.Click();
                Console.WriteLine("1st, 2nd, and 3rd checkboxes selected: ");
                Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));
            }

            if (!checkBox4.Selected)
            {
                checkBox4.Click();
                Console.WriteLine("1st, 2nd, 3rd, and 4th checkboxes selected: ");
                Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));
            }

            checkBox1.Click();
            Console.WriteLine("1st checkbox is deselected: ");
            Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));

            checkBox2.Click();
            Console.WriteLine("1st and 2nd checkbox deselected: ");
            Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));

            checkBox3.Click();
            Console.WriteLine("1st, 2nd, and 3rd checkbox deselected: ");
            Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));

            checkBox4.Click();
            Console.WriteLine("1st, 2nd, 3rd, and 4th checkbox deselected: ");
            Console.WriteLine("The button text: " + buttonchecktext.GetAttribute("value"));

            //return from Checkbox Demo to Main Page
            driver.Navigate().Back();
            System.Threading.Thread.Sleep(mysecs);

        }
    }

    public class Test102 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method1()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Radio Buttons Demo

            IWebElement InputForms3 = driver.FindElement(By.LinkText("Input Forms"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms3.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Radio Buttons Demo")).Click();

            //Verify expected text for male checkbox
            String out1 = "Radio button 'Male' is checked";

            IWebElement radio1 = driver.FindElement(By.XPath("(//label[@class='radio-inline'][contains(.,'Male')])[1]"));
            IWebElement checkbutton1 = driver.FindElement(By.Id("buttoncheck"));
            IWebElement checkmessage = driver.FindElement(By.XPath("//p[@class='radiobutton']"));

            System.Threading.Thread.Sleep(mysecs);
            radio1.Click();
            System.Threading.Thread.Sleep(mysecs);
            checkbutton1.Click();

            Assert.That(checkmessage.Text, Does.Contain(out1), "'{0}' does not contain '{1}'", checkmessage.Text, out1);

            if (checkmessage.Text.Contains(out1))
            {
                Console.WriteLine("The output expected: " + out1);
                Console.WriteLine("The output shown: " + checkmessage.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + out1);
                Console.WriteLine("The output shown: " + checkmessage.Text);
            }
        }

        [Test]
        public static void method2()
        {
            System.Threading.Thread.Sleep(mysecs);
            String out2 = "Radio button 'Female' is checked";
            IWebElement radio2 = driver.FindElement(By.XPath("(//label[@class='radio-inline'][contains(.,'Female')])[1]"));
            IWebElement checkbutton1 = driver.FindElement(By.Id("buttoncheck"));
            IWebElement checkmessage = driver.FindElement(By.XPath("//p[@class='radiobutton']"));

            System.Threading.Thread.Sleep(mysecs);
            radio2.Click();
            System.Threading.Thread.Sleep(mysecs);
            checkbutton1.Click();

            Assert.That(checkmessage.Text, Does.Contain(out2), "'{0}' does not contain '{1}'", checkmessage.Text, out2);

            if (checkmessage.Text.Contains(out2))
            {
                Console.WriteLine("The output expected: " + out2);
                Console.WriteLine("The output shown: " + checkmessage.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + out2);
                Console.WriteLine("The output shown: " + checkmessage.Text);
            }
        }

        [Test]
        public static void method3()
        {
            System.Threading.Thread.Sleep(mysecs);
            String abc1 = "Sex : Male";
            String abc2 = "Sex : Female";
            String abc3 = "Age group: 0 - 5";
            String abc4 = "Age group: 5 - 15";
            String abc5 = "Age group: 15 - 50";
            IWebElement sexmale = driver.FindElement(By.XPath("(//label[@class='radio-inline'][contains(.,'Male')])[2]"));
            IWebElement sexfemale = driver.FindElement(By.XPath("(//label[@class='radio-inline'][contains(.,'Female')])[2]"));
            IWebElement age0to5 = driver.FindElement(By.XPath("//input[@value='0 - 5']"));
            IWebElement age5to15 = driver.FindElement(By.XPath("//input[@value='5 - 15']"));
            IWebElement age15to50 = driver.FindElement(By.XPath("//input[@value='15 - 50']"));
            IWebElement checkbutton2 = driver.FindElement(By.XPath("//button[@type='button'][contains(.,'Get values')]"));
            IWebElement checkmessage2 = driver.FindElement(By.XPath("//p[@class='groupradiobutton']"));
            System.Threading.Thread.Sleep(mysecs);
            sexmale.Click();
            System.Threading.Thread.Sleep(mysecs);
            age0to5.Click();
            System.Threading.Thread.Sleep(mysecs);
            checkbutton2.Click();

            Assert.That(checkmessage2.Text, Does.Contain(abc1) & Does.Contain(abc3), "'{0}' does not contain '{1}'", checkmessage2.Text, abc1, abc3);

            if ((checkmessage2.Text.Contains(abc1)) && (checkmessage2.Text.Contains(abc3)))
            {
                Console.WriteLine("The output expected: " + "\n" + abc1 + "\n" + abc3);
                Console.WriteLine("The output shown: " + checkmessage2.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + "\n" + abc1 + "\n" + abc3);
                Console.WriteLine("The output shown: " + checkmessage2.Text);
            }
        }

