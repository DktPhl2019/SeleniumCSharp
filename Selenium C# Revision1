    public class Test125 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Data List Filter

            IWebElement InputForms26 = driver.FindElement(By.LinkText("List Box"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms26.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Data List Filter")).SendKeys(Keys.Enter);

            String n1name = "bre";
            String n2email = "test3";
            String n3phone = "555";

            IWebElement searchlist = driver.FindElement(By.XPath("//input[contains(@type,'search')]"));
            searchlist.SendKeys(n1name);
            System.Threading.Thread.Sleep(mysecs);
            searchlist.Clear();
            searchlist.SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            searchlist.SendKeys(n2email);
            System.Threading.Thread.Sleep(mysecs);
            searchlist.Clear();
            searchlist.SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            searchlist.SendKeys(n3phone);
            System.Threading.Thread.Sleep(mysecs);
            searchlist.Clear();
            searchlist.SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
        }
    }
}
