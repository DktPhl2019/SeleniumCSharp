            elementleft3.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft4.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft5.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright1.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright2.Click();
