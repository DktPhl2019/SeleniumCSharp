            System.Threading.Thread.Sleep(mysecs);
            search1.SendKeys(r1);
            System.Threading.Thread.Sleep(mysecs);
            search2.SendKeys(r2);
            System.Threading.Thread.Sleep(mysecs);
            search1.Clear();
            search1.SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            search2.Clear();
            search2.SendKeys(Keys.Enter);
            System.Threading.Thread.Sleep(mysecs);
            elementleft1.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft2.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft3.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft4.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft5.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright1.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright2.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright3.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright4.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementright5.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft1.Click();
            System.Threading.Thread.Sleep(mysecs);
            elementleft2.Click();
