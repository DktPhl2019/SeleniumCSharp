    public class Test124 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open JQuery List Box

            IWebElement InputForms25 = driver.FindElement(By.LinkText("List Box"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms25.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("JQuery List Box")).SendKeys(Keys.Enter);

