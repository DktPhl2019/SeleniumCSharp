    public class Test111 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Table Filter

            IWebElement InputForms12 = driver.FindElement(By.LinkText("Table"));
            InputForms12.SendKeys(Keys.Down);
            driver.FindElement(By.LinkText("Table Filter")).Click();

            IWebElement greenbtn = driver.FindElement(By.XPath("//button[contains(.,'Green')]"));
            IWebElement orangebtn = driver.FindElement(By.XPath("//button[contains(.,'Orange')]"));
            IWebElement redbtn = driver.FindElement(By.XPath("//button[contains(.,'Red')]"));
            IWebElement allbtn = driver.FindElement(By.XPath("//button[contains(.,'All')]"));
            IWebElement check1 = driver.FindElement(By.CssSelector(".table > tbody:nth-child(1) > tr:nth-child(1) > td:nth-child(3) > div:nth-child(1) > a:nth-child(1) > i:nth-child(1)"));
            IWebElement check2 = driver.FindElement(By.CssSelector(".table > tbody:nth-child(1) > tr:nth-child(2) > td:nth-child(3) > div:nth-child(1) > a:nth-child(1) > i:nth-child(1)"));
            IWebElement check3 = driver.FindElement(By.CssSelector(".table > tbody:nth-child(1) > tr:nth-child(3) > td:nth-child(3) > div:nth-child(1) > a:nth-child(1) > i:nth-child(1)"));
            IWebElement check4 = driver.FindElement(By.CssSelector(".table > tbody:nth-child(1) > tr:nth-child(4) > td:nth-child(3) > div:nth-child(1) > a:nth-child(1) > i:nth-child(1)"));
            IWebElement check5 = driver.FindElement(By.CssSelector(".table > tbody:nth-child(1) > tr:nth-child(5) > td:nth-child(3) > div:nth-child(1) > a:nth-child(1) > i:nth-child(1)"));

            System.Threading.Thread.Sleep(mysecs);

            greenbtn.Click();

            Console.WriteLine("Click Green Button: ");
            Console.WriteLine("check1 green should be shown: " + check1.Displayed);
            Console.WriteLine("check2 orange should be hidden: " + check2.Displayed);
            Console.WriteLine("check3 red should be hidden: " + check3.Displayed);
            Console.WriteLine("check4 green should be shown: " + check4.Displayed);
            Console.WriteLine("check5 orange should be hidden: " + check5.Displayed);

            System.Threading.Thread.Sleep(mysecs);

            orangebtn.Click();

            Console.WriteLine("Click Orange Button: ");
            Console.WriteLine("check1 green should be hidden: " + check1.Displayed);
            Console.WriteLine("check2 orange should be shown: " + check2.Displayed);
            Console.WriteLine("check3 red should be hidden: " + check3.Displayed);
            Console.WriteLine("check4 green should be hidden: " + check4.Displayed);
            Console.WriteLine("check5 orange should be shown: " + check5.Displayed);

            System.Threading.Thread.Sleep(mysecs);

            redbtn.Click();

            Console.WriteLine("Click Red Button: ");
            Console.WriteLine("check1 green should be hidden: " + check1.Displayed);
            Console.WriteLine("check2 orange should be hidden: " + check2.Displayed);
            Console.WriteLine("check3 red should be shown: " + check3.Displayed);
            Console.WriteLine("check4 green should be hidden: " + check4.Displayed);
            Console.WriteLine("check5 orange should be hidden: " + check5.Displayed);

            System.Threading.Thread.Sleep(mysecs);

            allbtn.Click();

            Console.WriteLine("Click All Button: ");
            Console.WriteLine("check1 green should be shown: " + check1.Displayed);
            Console.WriteLine("check2 orange should be shown: " + check2.Displayed);
            Console.WriteLine("check3 red should be shown: " + check3.Displayed);
            Console.WriteLine("check4 green should be shown: " + check4.Displayed);
            Console.WriteLine("check5 orange should be shown: " + check5.Displayed);

            System.Threading.Thread.Sleep(mysecs);
        }
    }

    public class Test112 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method1()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Table Sort & Search

            IWebElement InputForms13 = driver.FindElement(By.LinkText("Table"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms13.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Table Sort & Search")).Click();

            String mymessage1 = "Showing 1 to 32 of 32 entries";
            String mymessage2 = "Showing 1 to 25 of 32 entries";
            String mymessage3 = "Showing 1 to 10 of 32 entries";

            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).SendKeys("50");
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//select[contains(@name,'length')]")).Click();

            IWebElement mymessagebox = driver.FindElement(By.XPath("//div[@class='dataTables_info']"));

            Assert.That(mymessagebox.Text, Does.Contain(mymessage1), "'{0}' does not contain '{1}'", mymessagebox.Text, mymessage1);

            if (mymessagebox.Text.Contains(mymessage1))
            {
                Console.WriteLine("The output expected: " + mymessage1);
                Console.WriteLine("The output shown: " + mymessagebox.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + mymessage1);
                Console.WriteLine("The output shown: " + mymessagebox.Text);
            }

            System.Threading.Thread.Sleep(mysecs);
        }

        [Test]
        public static void method2()
        {
            System.Threading.Thread.Sleep(mysecs);
            String mymessage2 = "Showing 1 to 25 of 32 entries";
            IWebElement mymessagebox = driver.FindElement(By.XPath("//div[@class='dataTables_info']"));
