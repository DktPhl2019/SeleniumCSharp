    public class Test118 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Bootstrap Modals

            IWebElement InputForms19 = driver.FindElement(By.LinkText("Alerts & Modals"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms19.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Bootstrap Modals")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@href='#myModal0']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//a[@href='#'][contains(.,'Close')])[1]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@href='#myModal0']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//a[@href='#'][contains(.,'Save changes')])[1]")).Click();
            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//a[@href='#myModal']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@href='#myModal2']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//a[@href='#'][contains(.,'Close')])[3]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@href='#myModal2']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//a[@href='#'][contains(.,'Save changes')])[3]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@href='#myModal']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//a[@href='#'][contains(.,'Close')])[2]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//a[@href='#myModal']")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//a[@href='#'][contains(.,'Save changes')])[2]")).Click();
        }
    }

    public class Test119 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Window Popup Modal

            IWebElement InputForms20 = driver.FindElement(By.LinkText("Alerts & Modals"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms20.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Window Popup Modal")).Click();

            string current = driver.CurrentWindowHandle;
            PopupWindowFinder finder = new PopupWindowFinder(driver);
            string newHandle1 = finder.Click(driver.FindElement(By.XPath("//a[contains(.,'Follow On Twitter')]")));
            System.Threading.Thread.Sleep(mysecs);

            driver.SwitchTo().Window(newHandle1);
            System.Threading.Thread.Sleep(mysecs);
            driver.Close();

            System.Threading.Thread.Sleep(mysecs);
            driver.SwitchTo().Window(current);
            string newHandle2 = finder.Click(driver.FindElement(By.XPath("//a[contains(.,'Like us On Facebook')]")));
            System.Threading.Thread.Sleep(mysecs);
            driver.SwitchTo().Window(newHandle2);
            System.Threading.Thread.Sleep(mysecs);
            driver.Close();

            System.Threading.Thread.Sleep(mysecs);
            driver.SwitchTo().Window(current);

            string newHandle3 = finder.Click(driver.FindElement(By.XPath("//a[contains(.,'Follow Twitter & Facebook')]")));

            List<string> newHandle30 = driver.WindowHandles.ToList();

            foreach (string handle1 in newHandle30)
            {
                System.Threading.Thread.Sleep(mysecs);
                driver.SwitchTo().Window(handle1);
                if (driver.Title != "Selenium Easy - Window Popup Modal Demo")
                {
                    driver.Close();
                }
            }

            System.Threading.Thread.Sleep(mysecs);
            driver.SwitchTo().Window(current);

            string newHandle4 = finder.Click(driver.FindElement(By.XPath("//a[contains(.,'Follow All')]")));

            List<string> newHandle31 = driver.WindowHandles.ToList();

            foreach (string handle2 in newHandle31)
            {
                System.Threading.Thread.Sleep(mysecs);
                driver.SwitchTo().Window(handle2);
                if (driver.Title != "Selenium Easy - Window Popup Modal Demo")
                {
                    driver.Close();
                }
            }

            driver.SwitchTo().Window(current);
        }
    }

    public class Test120 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Progress Bar Modal

            IWebElement InputForms21 = driver.FindElement(By.LinkText("Alerts & Modals"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms21.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Progress Bar Modal")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//button[@type='button'])[2]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//button[@type='button'])[3]")).Click();
            System.Threading.Thread.Sleep(mysecs * 2);
            driver.FindElement(By.XPath("(//button[@type='button'])[4]")).Click();
            System.Threading.Thread.Sleep(mysecs * 2);
        }
    }

    public class Test121 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method1()
        {
            myMethod();
            //Open Javascript Alerts
            System.Threading.Thread.Sleep(mysecs);
            IWebElement InputForms22 = driver.FindElement(By.LinkText("Alerts & Modals"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms22.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Javascript Alerts")).SendKeys(Keys.Enter);

            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("//button[@class='btn btn-default']")).Click();
            System.Threading.Thread.Sleep(mysecs);

            IAlert alert11 = driver.SwitchTo().Alert();
            String a11 = alert11.Text;
            System.Threading.Thread.Sleep(mysecs);
            alert11.Accept();

            Console.WriteLine("The output: " + a11);

            System.Threading.Thread.Sleep(mysecs);

            driver.FindElement(By.XPath("(//button[contains(@class,'btn btn-default btn-lg')])[1]")).Click();
            System.Threading.Thread.Sleep(mysecs);

            IAlert alert12 = driver.SwitchTo().Alert();
            System.Threading.Thread.Sleep(mysecs);
            alert12.Dismiss();

            String m11 = "You pressed Cancel!";

            IWebElement alert2cancel = driver.FindElement(By.XPath("//p[contains(.,'You pressed Cancel!')]"));

            Assert.That(alert2cancel.Text, Does.Contain(m11), "'{0}' does not contain '{1}'", alert2cancel.Text, m11);

            if (alert2cancel.Text.Contains(m11))
            {
                Console.WriteLine("The output expected: " + m11);
                Console.WriteLine("The output shown: " + alert2cancel.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + m11);
                Console.WriteLine("The output shown: " + alert2cancel.Text);
            }

            System.Threading.Thread.Sleep(mysecs);
        }

        [Test]
        public static void method2()
        {
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("(//button[contains(@class,'btn btn-default btn-lg')])[1]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            IAlert alert13 = driver.SwitchTo().Alert();
            System.Threading.Thread.Sleep(mysecs);
            alert13.Accept();

            String m12 = "You pressed OK!";

            IWebElement alert2accept = driver.FindElement(By.XPath("//p[contains(.,'You pressed OK!')]"));

            Assert.That(alert2accept.Text, Does.Contain(m12), "'{0}' does not contain '{1}'", alert2accept.Text, m12);

            if (alert2accept.Text.Contains(m12))
            {
                Console.WriteLine("The output expected: " + m12);
                Console.WriteLine("The output shown: " + alert2accept.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + m12);
                Console.WriteLine("The output shown: " + alert2accept.Text);
            }
        }

        [Test]
        public static void method3()
        {
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.XPath("//button[@class='btn btn-default btn-lg'][contains(.,'Click for Prompt Box')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            IAlert alert14 = driver.SwitchTo().Alert();
            System.Threading.Thread.Sleep(mysecs);
            System.Threading.Thread.Sleep(mysecs);
            alert14.Dismiss();

            System.Threading.Thread.Sleep(mysecs);

            String m14 = "test";

            driver.FindElement(By.XPath("//button[@class='btn btn-default btn-lg'][contains(.,'Click for Prompt Box')]")).Click();
            System.Threading.Thread.Sleep(mysecs);
            IAlert alert15 = driver.SwitchTo().Alert();
            System.Threading.Thread.Sleep(mysecs);
            alert15.SendKeys(m14);
            System.Threading.Thread.Sleep(mysecs);
            alert15.Accept();

            IWebElement popuptext = driver.FindElement(By.Id("prompt-demo"));

            String m15 = "You have entered" + "'" + m14 + "'!";

            Assert.That(popuptext.Text, Does.Contain(m15), "'{0}' does not contain '{1}'", popuptext.Text, m15);

            if (popuptext.Text.Contains(m15))
            {
                Console.WriteLine("The output expected: " + m15);
                Console.WriteLine("The output shown: " + popuptext.Text);
            }
            else
            {
                Console.WriteLine("The output expected: " + m15);
                Console.WriteLine("The output shown: " + popuptext.Text);
            }
        }
    }

    public class Test122 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()

        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Javascript Alerts

            IWebElement InputForms23 = driver.FindElement(By.LinkText("Alerts & Modals"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms23.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("File Download")).SendKeys(Keys.Enter);

            String myinputtext = "Transportation moves people and goods from one place to another using a variety of vehicles.";

            IWebElement inputtext = driver.FindElement(By.Id("textbox"));
            System.Threading.Thread.Sleep(mysecs);
            inputtext.SendKeys(myinputtext);
            System.Threading.Thread.Sleep(mysecs);

            IWebElement generatefile = driver.FindElement(By.Id("create"));
            System.Threading.Thread.Sleep(mysecs);
            generatefile.Click();
            System.Threading.Thread.Sleep(mysecs);

            IWebElement downloadfile = driver.FindElement(By.XPath("(//a[contains(.,'Download')])[7]"));
            downloadfile.Click();
            System.Threading.Thread.Sleep(mysecs);
        }
    }

    public class Test123 : InitialSetup
    {
        public static void myMethod()
        {
            driver.Navigate().GoToUrl("https://demo.seleniumeasy.com/");
            driver.Manage().Window.Maximize();
        }

        [Test]
        public static void method()
        {
            myMethod();
            System.Threading.Thread.Sleep(mysecs);
            //Open Bootstrap List Box

            IWebElement InputForms24 = driver.FindElement(By.LinkText("List Box"));
            System.Threading.Thread.Sleep(mysecs);
            InputForms24.SendKeys(Keys.Down);
            System.Threading.Thread.Sleep(mysecs);
            driver.FindElement(By.LinkText("Bootstrap List Box")).SendKeys(Keys.Enter);

            String r1 = "a";
            String r2 = "b";

            IWebElement search1 = driver.FindElement(By.XPath("(//input[contains(@type,'text')])[1]"));
            IWebElement search2 = driver.FindElement(By.XPath("(//input[contains(@type,'text')])[2]"));

            IWebElement elementleft1 = driver.FindElement(By.XPath("//li[@class='list-group-item'][contains(.,'bootstrap-duallist')]"));
            IWebElement elementleft2 = driver.FindElement(By.XPath("(//li[@class='list-group-item'][contains(.,'Dapibus ac facilisis in')])[1]"));
            IWebElement elementleft3 = driver.FindElement(By.XPath("(//li[@class='list-group-item'][contains(.,'Morbi leo risus')])[1]"));
